{
  "buildCommand": "prisma generate --no-engine && next build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "next dev",
  "build": {
    "env": {
      "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  }
}
